{
    "css.validate": false,
    "scss.validate": false,
    "less.validate": false,
    "css.customData": [
        ".vscode/css_custom_data.json"
    ],
    "tailwindCSS.experimental.classRegex": [
        [
            "cva\\(([^)]*)\\)",
            "[\"'`]([^\"'`]*).*?[\"'`]"
        ],
        [
            "cx\\(([^)]*)\\)",
            "(?:'|\"|`)([^']*)(?:'|\"|`)"
        ]
    ],
    "tailwindCSS.includeLanguages": {
        "typescript": "javascript",
        "typescriptreact": "javascript"
    },
    "files.associations": {
        "*.css": "tailwindcss"
    }
}
